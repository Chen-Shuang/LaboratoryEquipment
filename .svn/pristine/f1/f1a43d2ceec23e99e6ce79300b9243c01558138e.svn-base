<script>
whjc(1);				//初始化最前面的完好检查表
function whjc(x){
	$.post("WHJC/whjc",{"x":x},function(data){
		whjc_trList(data.list);
	});
};

function whjc_trList(data) {			//需要的是list对象
	var dataStr = "";
	$.each(data, function(i, v) {
		dataStr += whjc_stringBuilder(v,i);
	});
	// 把拼接好的表格内容填充到指定位置
	$("#whjc_body").html(dataStr);						//这里是正式填充的语句！！！！！

};
function whjc_stringBuilder(v,i) {
	i=i+1;
	var dataStr = "";
	dataStr += '<tr onclick="B001_click(\''+v.t_sbbh+'\',\''+v.t_sbmc+'\',\''+v.t_sblx+'\',\''+v.t_ggxh+'\',\''+v.t_sybm+'\')">';
	dataStr += '<td>' + i + '</td>';
	dataStr += '<td>' + v.t_sbbh + '</td>';
	dataStr += '<td>' + v.t_sbmc + '</td>';
	dataStr += '<td>' + v.t_sybm + '</td>';
	dataStr += '<td>' + v.t_jcrq + '</td>';
	dataStr += '<td>' + v.t_jcjg + '</td>';
	dataStr += '<td>' + v.t_jcr + '</td>';
	dataStr += '<td><a class="blue" href="javascript:ckxx(\''+v.id+'\')"><i class="ace-icon fa fa-search-plus bigger-130"></i></a>';
	dataStr += '<a class="red" href="javascript:scxx(\''+v.t_id+'\')"><i class="ace-icon fa fa-trash-o bigger-130" style="margin-left:10px;"></i></a></td>';
	dataStr += '</tr>';
	return dataStr;
};
